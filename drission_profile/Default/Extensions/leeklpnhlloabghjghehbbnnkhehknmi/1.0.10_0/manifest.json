{
   "action": {
      "default_icon": {
         "16": "assets/images/honor_SafeInternet_16.png",
         "19": "assets/images/honor_SafeInternet_19.png",
         "32": "assets/images/honor_SafeInternet_32.png",
         "38": "assets/images/honor_SafeInternet_38.png"
      }
   },
   "background": {
      "service_worker": "qax-security/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "qax-security/blockui.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "zh_CN",
   "description": "拦截恶意网址，净化上网环境",
   "host_permissions": [ "http://*/*", "https://*/*" ],
   "icons": {
      "128": "assets/images/honor_SafeInternet_128.png",
      "256": "assets/images/honor_SafeInternet_256.png",
      "48": "assets/images/honor_SafeInternet_48.png",
      "512": "assets/images/honor_SafeInternet_512.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "荣耀安全上网插件",
   "permissions": [ "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.10",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "*" ]
   } ]
}
