{
   "action": {
      "default_icon": {
         "16": "images/logo16.png",
         "32": "images/logo32.png"
      }
   },
   "author": "Tonec FZE",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extension_desc__",
   "externally_connectable": {
      "matches": [ "*://*.internetdownloadmanager.com/*", "*://*.tonec.com/*" ]
   },
   "homepage_url": "http://www.internetdownloadmanager.com/",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "images/logo128.png",
      "16": "images/logo16.png",
      "48": "images/logo48.png"
   },
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "109.0",
   "name": "IDM Integration Module",
   "optional_permissions": [ "notifications", "system.display" ],
   "permissions": [ "scripting", "tabs", "cookies", "contextMenus", "webNavigation", "webRequest", "declarativeNetRequest", "downloads", "downloads.shelf", "downloads.ui", "management", "storage", "proxy", "nativeMessaging" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "6.42.50",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "document.js", "debug.js", "captured.js", "captured.html" ],
      "use_dynamic_url": false
   } ]
}
